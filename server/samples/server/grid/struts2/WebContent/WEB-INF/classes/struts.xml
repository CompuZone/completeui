<struts>
    <package name="com.nitobi.struts2.example" extends="nitobi-default">
    	<action name="GetHandler" class="examples.grid.struts2.GetHandlerAction">
    		<result name="success" type="nitobi">
    			<param name="handlername">gethandler</param>
    		</result>
    	</action>
    	
