<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>java</display-name>
	<servlet>
		<servlet-name>GetCustomersHandler</servlet-name>
		<servlet-class>examples.treegrid.betademo.GetCustomersHandler</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>GetCustomersHandler</servlet-name>
		<url-pattern>/treegrid/java/betademo/get</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetOrdersHandler</servlet-name>
		<servlet-class>examples.treegrid.betademo.GetOrdersHandler</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>GetOrdersHandler</servlet-name>
		<url-pattern>/treegrid/java/betademo/getorders</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SaveOrdersHandler</servlet-name>
		<servlet-class>examples.treegrid.betademo.SaveOrdersHandler</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SaveOrdersHandler</servlet-name>
		<url-pattern>/treegrid/java/betademo/saveorders</url-pattern>
	</servlet-mapping>
</web-app>